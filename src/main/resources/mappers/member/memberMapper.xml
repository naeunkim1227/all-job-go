<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.web.alljobgo.userMapper">
  	<insert id="signUser">
  		insert into member_info (id,pass,name,phonenumber,address,fav_Area,fav_Cate)
  		values (#{id},#{pass},#{name},#{phonenumber},concat(ifnull(#{address},''),ifnull(#{address_detail},'')),#{fav_Area},#{fav_Cate})
  	</insert>
  </mapper>